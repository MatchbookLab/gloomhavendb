    "tslint": "^6.1.2",
    "typescript": "~3.8",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "x-ray": "^2.3.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:quick"
    }
  }
}
