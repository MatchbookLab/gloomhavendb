{
  "name": "gloomhavendb",
  "description": "Gloomhaven DB",
  "author": "CWSpear <cameron@matchbooklab.com>",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/server/main.js",
    "start:dev": "npm run webpack:server:once && npm-run-all --parallel start:client webpack:server start:server:hmr",
    "start:client": "ng serve --configuration hmr --host 0.0.0.0",
    "start:server": "nodemon --watch ./src/server --watch shared --ext ts --exec 'npm run start:server:once'",
    "start:server:once": "ts-node --project ./src/server/tsconfig.server.json -r tsconfig-paths/register ./src/server/main.ts",
    "start:server:hmr": "node ./dist/server-hot/server",
    "start:server:dev": "npm run webpack:server:once && npm-run-all --parallel webpack:server start:server:hmr",
    "build:client": "ng build",
    "build:prod:client": "ng build --prod --aot",
    "build:prod:universal": "ng run gloomhavendb:universal",
    "build:prod:server": "tsc --project src/server/tsconfig.server.json",
    "build:prod": "npm run clean && npm-run-all --parallel build:prod:**",
    "test": "npm run test:client:cov && npm run test:server:cov",
    "test:client": "jest --config src/client/jest.config.js",
    "test:client:cov": "npm run test:client -- --coverage",
    "test:server": "jest --config src/server/jest.config.js",
    "test:server:integration": "jest --runInBand --config src/server/jest.config.integration.js",
    "test:server:integration:cov": "npm run test:server:integration -- --coverage",
    "test:server:cov": "npm run test:server -- --coverage",
    "webpack:server": "webpack --config webpack.config.js",
    "webpack:server:once": "npm run clean && (NO_WATCH=1 webpack --config webpack.config.js || exit 0)",
    "mailhog": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up mailhog",
    "clean": "rm -rf dist/server",
    "lint": "ng lint",
    "e2e": "(cd e2e && npm test)",
    "e2e:dev": "(cd e2e && npm run test:dev)",
    "format": "prettier --list-different --write \"{src,e2e}/**/*.{ts,html,scss}\"",
    "format:quick": "pretty-quick --staged --pattern \"{src,e2e}/**/*.{ts,html,scss}\"",
    "format:ci": "prettier --check \"{src,e2e}/**/*.{ts,html,scss}\"",
    "cli:sdk": "ts-node -P src/cli/tsconfig.json src/cli/sdk-generator.ts",
    "cli:service": "ts-node -P src/cli/tsconfig.json src/cli/service-generator.ts",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/platform-server": "^9.1.9",
    "@angular/pwa": "^0.901.7",
    "@angular/router": "^9.1.9",
    "@angular/service-worker": "^9.1.9",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@nestjs/common": "^7.0.13",
    "@nestjs/core": "^7.0.13",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/ng-universal": "^3.1.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.13",
    "@nestjs/swagger": "^4.5.7",
    "@nestjs/typeorm": "^7.0.0",
    "@nguniversal/common": "^9.1.1",
    "@nguniversal/express-engine": "^9.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "angular-svg-icon": "^9.2.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "diff": "^4.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "forcedomain": "^2.0.0",
    "helmet": "^3.22.0",
    "lodash": "^4.17.15",
    "logrocket": "^1.0.7",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.6",
    "nodemailer-mailgun-transport": "^2.0.0",
    "normalize.css": "^8.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
